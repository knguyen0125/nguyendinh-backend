{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "name": "newsItem"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "metadata": {
      "type": "component",
      "repeatable": false,
      "component": "seo.metadata",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "section.section-break",
        "section.text-image-section"
      ]
    },
    "news_types": {
      "collection": "news-type",
      "via": "news_items",
      "dominant": true
    },
    "banners": {
      "collection": "banner"
    },
    "head": {
      "type": "component",
      "repeatable": false,
      "component": "section.text-image-section",
      "required": true
    }
  }
}
